PosProxy
  Name "Wand Proxy"
  device { "SimWandPos" }
  unit { "0" }
  etrans { "True" }
  translate { "0.0" "0.0" "0.0" }
  rotate { "0.0" "0.0" "0.0" }
  useFilter { "False" }
  sigmoidParams { 
      sigmoid_linear_pos_filter
        Name "sigmoidParams 0"
        minDist { "0.05" }
        maxDist { "0.2" }
        maxThreshold { "0.0" }
        end
 }
  end

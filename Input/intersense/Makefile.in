# -----------------------------------------------------------------------------
# Makefile.in for juggler/Input/intersense.  It requires GNU make.
# modified from ibox Makefile.in by Chris Johanson 17/9/00
#
# Generated for use on @PLATFORM@
# -----------------------------------------------------------------------------

default: all

# Include common definitions.
include @topdir@/Makefile.base

includedir	= @includedir@/Input/intersense
srcdir		= @srcdir@
top_srcdir	= @top_srcdir@
INSTALL		= @INSTALL@
VJROOT		= ${top_srcdir}
VJ_SRCDIR	= @VJ_SRCDIR@/Input/intersense
WORKDIR		= ${topdir}/Input/intersense

# Include source building, dependency and installation targets.
include ${MKPATH}/vj.compile.mk
include ${MKPATH}/vj.dep.mk
include ${MKPATH}/vj.install.mk

# Search path for source code and object files.
vpath
vpath %.c @srcdir@
vpath %.C @srcdir@
vpath %.${OBJ_FILE_SUFFIX} ${OBJDIR}

INTERSENSE_SRCS	:= isdriver.c	\
                   isense.c 	\
                   itcom.c 	\
                   timer.c 	\
                   serial.c

INTERSENSE_OBJECTS	:= ${addprefix ${OBJDIR}/,${INTERSENSE_SRCS:.c=.${OBJ_FILE_SUFFIX}}}
DEPEND_FILES	:= ${INTERSENSE_SRCS:.c=.d}

# -----------------------------------------------------------------------------
# Library targets.
# -----------------------------------------------------------------------------
all dbg opt dso ddso obj: ${INTERSENSE_OBJECTS}

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
CLEAN_FILES	+= ${INTERSENSE_OBJECTS}

# -----------------------------------------------------------------------------
# Include dependencies generated by make automatically.
# -----------------------------------------------------------------------------
ifndef DO_CLEANDEPEND
    -include ${DEPEND_FILES}
endif

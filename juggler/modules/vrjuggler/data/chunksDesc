  Name String 1 "Name" "Unique name of an instance of this chunk type"
  modKey Int 1 "modKey" vj_valuelabels { "Forward" "Back" "Left" "Right" "Up" "Down" "Rot Right" "Rot Left" "Rot Up" "Rot Down" "Roll CCW" "Roll CW" } vj_enumeration { "NONE=0" "SHIFT=5" "CTRL=6" "ALT=7" } "Modifier keys used for moving the position"

chunk Flock "Flock" ""
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  port String 1 "port" ""
  baud Int 1 "baud" ""
  num Int 1 "Num Birds" "Number of birds"
  translate Float 3 "translate" ""
  rotate Float 3 "rotate" "x y z rotations to transform tracker coord sys to C2 coord sys"
  calfile String 1 "calfile" ""
  hemi Int 1 "hemi" vj_enumeration { "FRONT_HEM=0" "AFT_HEM=1" "UPPER_HEM=2" "LOWER_HEM=3" "LEFT_HEM=4" "RIGHT_HEM=5" } "Flock Hemisphere"
  filt Int 1 "filt" vj_enumeration { "AC_NARROW=0" "AC_WIDE=1" "DC_FILTER=2" } "Filter"
  sync Int 1 "sync" "sync style 0-(nosync) 1-(normal 72Hz) see flock man pg 82"
  blocking Int 1 "blocking" "blocked port? 0-no 1-yes"
  report String 1 "report" "Q-(every cycle) R-(every other cycle) S-(every 8 cycles) T-(every 32 cycles)"
  transmitter Int 1 "transmitter" "Id of the transmitter"
  end

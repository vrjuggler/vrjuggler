  rotate Float 3 "Rotation" ""
  ipaddress String 1 "IP Address" vj_enumeration { "Backpack 1" "Backpack 2" } ""
  end
chunk SimRelativePosition "SimRelativePosition" "A positional device simulator for one device relative to another."
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  base_frame_proxy Chunk 1 "base_frame_proxy" vj_enumeration { "PosProxy" } "Pos proxy that devices the base frame of reference"
  relative_proxy Chunk 1 "relative_proxy" vj_enumeration { "PosProxy" } "device that is relative to the base frame"
  end

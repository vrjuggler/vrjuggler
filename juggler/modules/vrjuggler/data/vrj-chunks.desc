chunk displaySystem "Display System" ""
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  numpipes Int 1 "Number of Pipes" "The number of pipes on the system"
  xpipes String 8 "xpipes" "The x-display strings for the pipes"
  end
chunk IBox "IBox" "Configuration for the IBox"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  port String 1 "Port" "Serial port this device is connected to"
  portnum Int 1 "portnum" ""
  baud Int 1 "Baud Rate" "Serial port speed"
  end
chunk PinchGlove "PinchGlove" "Configuration for the PinchGlove"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  port String 1 "Port" "Serial port that the PinchGlove is connected to"
  baud Int 1 "Baud Rate" "Serial port speed (9600)"
  glovePos Chunk 1 "Glove Position" vj_enumeration { "proxyAlias" "PosProxy" } "The position proxy for the glove position. i.e. the tracker on the glove."
  end
chunk CyberGlove "CyberGlove" "Configuration for the CyberGlove"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  port String 1 "Port" "Serial port that the CyberGlove is connected to"
  baud Int 1 "Baud Rate" "Serial port speed"
  calDir String 1 "Calibration Dir" "Home directory for calibration files"
  glovePos Chunk 1 "Glove Position" vj_enumeration { "proxyAlias" "PosProxy" } "The position proxy for the glove position. i.e. the tracker on the glove."
  end
chunk DummyPosition "Dummy Position" "A 'dummy' position device"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  position Float 3 "Position" vj_valuelabels { "x" "y" "z" } "Initial Dummy Position"
  ori Float 3 "Orientation" vj_valuelabels { "x" "y" "z" } "Initial Dummy Orientation XYZ Euler"
  end
chunk ThreeDMouse "3D Mouse" "Configuration for Logitech 3d mouse"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  port String 1 "Port" "Serial port this device is connected to"
  baud Int 1 "Baud Rate" "Serial port speed"
  end
chunk proxyAlias "Proxy Alias" "An additional name for referencing a device proxy"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  aliasName String 1 "aliasName" "The string alias to use."
  proxyPtr Chunk 1 "Proxy Pointer" vj_enumeration { "PosProxy" "DigProxy" "AnaProxy" "GloveProxy" "KeyboardProxy" "GestureProxy" } "The proxy this alias refers to"
  end
chunk AnaProxy "Analog Proxy" "Used to access a single analog input source"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  device Chunk 1 "Device" vj_enumeration { "SimAnalog" "IBox" } "The device this proxy attaches to"
  unit Int 1 "Unit" "The unit in the device this proxy attaches to"
  end
chunk DigProxy "Digital Proxy" "Used to access a single digital (binary) input source"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  device Chunk 1 "Device" vj_enumeration { "SimDigital" "IBox" "PinchGlove" "ThreeDMouse" } "The device this proxy attaches to"
  unit Int 1 "Unit" "The unit in the device this proxy attaches to"
  end
chunk apiPerformer "Performer API" "This is information for configuring the Performer API.  It only pertains to Performer."
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  simHeadModel String 1 "SimHeadModel" "This is the full path to the model used for the head."
  simWandModel String 1 "SimWandModel" "This is the full path to the model used as a wand."
  end
chunk PosProxy "Position Proxy" "Used to access a single positional input source"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  device Chunk 1 "device" vj_enumeration { "Flock" "SimPosition" "DummyPosition" "motionstar" "vjSimRelativePosition" } "The device the proxy points to"
  unit Int 1 "unit" "The unit in the device this proxy attaches to"
  etrans Bool 1 "Enable Transformation" "If true, the Proxy Transformation is applied to data"
  translate Float 3 "Translation" vj_valuelabels { "Trans X" "Trans Y" "Trans Z" } "Proxy translation (in the reciever's coord system)"
  rotate Float 3 "Rotation" vj_valuelabels { "X" "Y" "Z" } "Proxy rotation (Euler angles in the reciever's coord system)"
  end
chunk InputManager "Input Manager" "Configure the initial Input Manager settings."
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  dummyPos Float 3 "Dummy Position" vj_valuelabels { "X" "Y" "Z" } "The Position of the dummy value."
  dummyOr Float 3 "Dummy Orientation" vj_valuelabels { "X" "Y" "Z" } "The Euler XYZ orientation of the dummy"
  end
chunk Flock "Flock of Birds" "Configuration for Ascension Flock of Birds"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  port String 1 "Port" "Serial port the Flock is attached to"
  baud Int 1 "Baud Rate" "Serial port speed"
  num Int 1 "Num Birds" "Number of birds in the Flock"
  translate Float 3 "Translation" vj_valuelabels { "X" "Y" "Z" } "Translation used to get pt from tracker coordinates to Juggler coordinate system. world_T_transmitter = T*R*pt"
  rotate Float 3 "Rotation" vj_valuelabels { "X" "Y" "Z" } "Rotation used to get pt from tracker coordinates to Juggler coordinate system. world_T_tranmitter = T*R*pt"
  calfile String 1 "Calibration file" "Name of a tracker calibration file"
  hemi Int 1 "Hemisphere" vj_enumeration { "FRONT_HEM=0" "AFT_HEM=1" "UPPER_HEM=2" "LOWER_HEM=3" "LEFT_HEM=4" "RIGHT_HEM=5" } "Flock Hemisphere"
  filt Int 1 "Filter" vj_enumeration { "AC_NARROW=0" "AC_WIDE=1" "DC_FILTER=2" } "Filter"
  sync Int 1 "Synchronous" vj_enumeration { "NoSync=0" "Normal_72Hz=1" } "sync style. See flock man pg 82"
  blocking Int 1 "Blocking" "blocked port? 0-no 1-yes"
  report String 1 "Report Frequency" "Q-(every cycle) R-(every other cycle) S-(every 8 cycles) T-(every 32 cycles)"
  transmitter Int 1 "Transmitter" "Id of the transmitter"
  end
chunk Keyboard "Keyboard" "Configures a window that accepts keyboard & mouse input"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  width Int 1 "width" "window width"
  height Int 1 "height" "window height"
  origin Int 2 "origin" vj_valuelabels { "X" "Y" } "Window origin (upper left is 0,0)"
  msens Float 1 "msens" "Mouse sensitivity, 1 is 1 keypress per pixel moved, 0.1 is 1 key for 10 pixels"
  end
chunk GloveProxy "Glove Proxy" "A Glove Proxy points to a glove device and unit"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  device Chunk 1 "Device" vj_enumeration { "CyberGlove" "SimGloveGesture" "PinchGlove" } "The glove device the proxy connects to."
  unit Int 1 "Unit" "The unit number in the device to hook to."
  visible Bool 1 "Visible" "If true, the glove will be rendered in display windows."
  end
chunk KeyboardProxy "Keyboard Proxy" "This proxy points to a keyboard device."
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  device Chunk 1 "Device" vj_enumeration { "Keyboard" "simKeyboardDigital" } "The keyboard device to hook the proxy to."
  end
chunk SimGloveGesture "Glove Gesture Simulator" "Simulate glove gesture device"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  keyboardProxy Chunk 1 "Keyboard Proxy" vj_enumeration { "KeyboardProxy" } "The keyboard to use."
  keyPairs EmbeddedChunk -1 "Key Presses" vj_enumeration { "KeyModPair" } ""
  trainedFilename String 1 "Trained File Name" "The name of the trained gesture file.  Used to get simulated values."
  glovePos Chunk 1 "Glove Position" vj_enumeration { "PosProxy" } "The position proxy for the glove position."
  end
chunk GestureProxy "Gesture Proxy" "A Gesture Proxy points to a gesture device."
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  device Chunk 1 "Device" vj_enumeration { "SimGloveGesture" } "The gesture device to hook the proxy to."
  end
chunk SimAnalog "Analog Simulator" "Simulated analog device.  Gets input from keyboard proxy."
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  keyboardProxy Chunk 1 "Keyboard Proxy" vj_enumeration { "KeyboardProxy" } "The keyboard to use for event input."
  keyPairsInc EmbeddedChunk -1 "Increment Keypress" vj_enumeration { "KeyModPair" } "Keys for moving value up"
  keyPairsDec EmbeddedChunk -1 "Decrement Keypress" vj_enumeration { "KeyModPair" } "Keys for moving value down"
  anastep Int 1 "Delta" "The amount that the analog data changes per keypress"
  end
chunk SimDigital "Digital Simulator" "Simulated digital device.  Gets input from keyboard proxy."
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  keyboardProxy Chunk 1 "Keyboard Proxy" vj_enumeration { "KeyboardProxy" } "The keyboard to use for event input."
  keyPairs EmbeddedChunk -1 "KeyPresses" vj_enumeration { "KeyModPair" } "Keypresses to trigger multiple digital inputs"
  end
chunk SimPosition "Position Simulator" "A simulated position device. Gets input from a keyboard proxy"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  keyboardProxy Chunk 1 "Keyboard Proxy" vj_enumeration { "KeyboardProxy" } "The keyboard to use for event input."
  keyPairs EmbeddedChunk 12 "keyPairs" vj_valuelabels { "Forward" "Back" "Left" "Right" "Up" "Down" "Rot Right" "Rot Left" "Rot Up" "Rot Down" "Rot CCW" "Rot CW" } vj_enumeration { "KeyModPair" } "Keys for moving and rotating the simulated device."
  initialPos Float 3 "Initial Position" vj_valuelabels { "X" "Y" "Z" } "Initial position"
  initialRot Float 3 "Initial Rotation" vj_valuelabels { "X" "Y" "Z" } "Initial Euler rotation"
  dtrans Float 1 "Translation Delta" "How much to translate for a single keypress."
  drot Float 1 "Rotation Delta" "How many degrees to rotate for a single keypress."
  transCoordSystem Int 1 "Translation Coordinate System" vj_enumeration { "Local=0" "Global=1" } "Do the translations apply to the tracker's local coordinate system?  Ex. Wand - global, Camera - Local"
  rotCoordSystem Int 1 "Rotation Coordinate System" vj_enumeration { "Local=0" "Global=1" } "Do the rotations apply to the tracker's local coordinate system?"
  end
chunk EnvironmentManager "Environment Manager" "Application-side of connection to vjcontrol GUI"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  Port Int 1 "Port" "Networking port to listen on"
  AcceptConnections Bool 1 "Accept Connections" "Whether or not we should accept connections from vjcontrol clients"
  PerformanceTarget Chunk 1 "Performance Target" vj_enumeration { "FileConnect" "NetworkConnect" } "File or network connection that performance data should be sent to."
  end
chunk FileConnect "EM Connection" "A file for outputing Environment Manager data"
  Name String 1 "Name" "Unique name of an instance of this chunk type"
  FileName String 1 "File Name" ""
  Enabled Bool 1 "Enabled" "Should file be opened & used?"

VPATH	= @srcdir@

testPlane_OBJS	= testPlane.@OBJEXT@

testSeg_OBJS	= testSeg.@OBJEXT@

# -----------------------------------------------------------------------------
# Application build targets.
# -----------------------------------------------------------------------------
all: testPlane@EXEEXT@ testSeg@EXEEXT@

testPlane@EXEEXT@: $(testPlane_OBJS)

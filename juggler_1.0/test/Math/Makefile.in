# Suffix rules for building object files.
.SUFFIXES: .cpp .@OBJEXT@

.cpp.@OBJEXT@:
	$(CXX_COMPILE) @OBJ_NAME_FLAG@ @OBJ_BUILD_FLAG@ $<

# -----------------------------------------------------------------------------
# Clean-up targets.
# -----------------------------------------------------------------------------
clean:
	rm -f Makedepend *.@OBJEXT@ testPlane.ilk testSeg.ilk  so_locations *.?db core*
	rm -rf ii_files

clobber:
	@$(MAKE) clean
	rm -f testPlane@EXEEXT@ testSeg@EXEEXT@ 

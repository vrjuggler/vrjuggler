# -----------------------------------------------------------------------------
# Makefile.in for vrjuggler/test/Input
#
# Generated for use on @PLATFORM@
# -----------------------------------------------------------------------------

# Generated by mkmakefile.pl using the following options:
#    --apps aFlockTest=aFlockTest.cpp,FlockStandalone.cpp --apps
#    aMotionStarTest=aMotionStarTest.cpp,MotionStarStandalone.cpp --apps
#    dummyTrackd=dummyTrackd.cpp --apps
#    fsPinchGloveTest=fsPinchGloveTest.cpp --apps go=main.cpp --apps
#    go-ibox=main-ibox.cpp --apps go-inputgroup=main-inputgroup.cpp --apps
#    go-logiclass=main-logiclass.cpp --apps FlockTest=FlockTest.cpp
#    --srcdir=../../vrj/Input/Devices/Ascension
#    --extralibs=@APP_EXTRA_LIBS_X11@
#
# NOTE:
#    Modified by hand to change libraries required for aFlockTest and
#    aMotionStarTest.

default: all

# Basic options.
srcdir		= @srcdir@
CFLAGS		= @APP_CFLAGS@ $(EXTRA_CFLAGS) $(INCLUDES) $(DEFS)
CXXFLAGS	= @APP_CXXFLAGS@ $(EXTRA_CFLAGS) $(INCLUDES) $(DEFS)
DEFS		= @APP_DEFS@
EXTRA_CFLAGS	= @APP_EXTRA_CFLAGS@ $(DEBUG_CFLAGS)
DEBUG_CFLAGS	= @APP_DEBUG_CFLAGS@
OPTIM_CFLAGS	= @APP_OPTIM_CFLAGS@
INCLUDES	= @APP_INCLUDES@ -I$(srcdir) -I$(srcdir)/../	\
		  -I$(srcdir)/../gadget/Devices/Ascension 	\
		  -I$(srcdir)/../drivers/Elexol/Ether24 	\
		  -I$(srcdir)/../drivers/Polhemus/Fastrak


EXTRA_LFLAGS	= @APP_EXTRA_LFLAGS@ $(DEBUG_LFLAGS)
DEBUG_LFLAGS	= @APP_DEBUG_LFLAGS@
OPTIM_LFLAGS	= @APP_OPTIM_LFLAGS@
LINK_FLAGS	= @APP_LINK_FLAGS@ $(EXTRA_LFLAGS)

# Libraries needed for linking.
BASIC_LIBS	= @APP_BASIC_LIBS_BEGIN@ @APP_BASIC_LIBS@  @APP_BASIC_EXT_LIBS@ @APP_BASIC_LIBS_END@
EXTRA_LIBS	= @APP_EXTRA_LIBS_BEGIN@ @APP_EXTRA_LIBS@ @APP_EXTRA_LIBS_X11@ @APP_EXTRA_LIBS_END@ 

# Commands to execute.
C_COMPILE	= @APP_CC@ $(CFLAGS)
CXX_COMPILE	= @APP_CXX@ $(CXXFLAGS)
LINK		= @APP_LINK@ $(LINK_FLAGS)
LINKALL_ON	= @APP_LINKALL_ON@
LINKALL_OFF	= @APP_LINKALL_OFF@

VPATH	= @srcdir@:					\
	  @srcdir@/../drivers/Ascension/Flock:		\
	  @srcdir@/../drivers/Fakespace/PinchGlove:	\
	  @srcdir@/../drivers/Immersion/IBox:		\
	  @srcdir@/../drivers/Elexol/Ether24:		\
	  @srcdir@/../drivers/Polhemus/Fastrak


ElexolTest_OBJS	= Ether24Standalone.@OBJEXT@ ElexolTest.@OBJEXT@

FastrakTest_OBJS = FastrakStandalone.@OBJEXT@ FastrakTest.@OBJEXT@

aFlockTest_OBJS	= FlockStandalone.@OBJEXT@ aFlockTest.@OBJEXT@

flockDebug_OBJS	= FlockStandalone.@OBJEXT@ flockDebug.@OBJEXT@

aMotionStarTest_OBJS	= MotionStarStandalone.@OBJEXT@ aMotionStarTest.@OBJEXT@

IBoxTest_OBJS	= IBoxStandalone.@OBJEXT@ IBoxTest.@OBJEXT@

dummyTrackd_OBJS	= dummyTrackd.@OBJEXT@

fsPinchGloveTest_OBJS	= fsPinchGloveTest.@OBJEXT@

gadgetTest_OBJS	= gadgetTest.@OBJEXT@ PinchGloveAdaptor.@OBJEXT@ IboxAdaptor.@OBJEXT@ FlockAdaptor.@OBJEXT@ BaseAdaptor.@OBJEXT@

go_OBJS	= main.@OBJEXT@

go-ibox_OBJS	= main-ibox.@OBJEXT@

go-inputgroup_OBJS	= main-inputgroup.@OBJEXT@

go-logiclass_OBJS	= main-logiclass.@OBJEXT@

FlockTest_OBJS	= FlockTest.@OBJEXT@

# -----------------------------------------------------------------------------
# Application build targets.
# -----------------------------------------------------------------------------
all: gadgetTest

ElexolTest@EXEEXT@: $(ElexolTest_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(ElexolTest_OBJS) @APP_EXTRA_LIBS@

FastrakTest@EXEEXT@: $(FastrakTest_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(FastrakTest_OBJS) @APP_EXTRA_LIBS@
	
aFlockTest@EXEEXT@: $(aFlockTest_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(aFlockTest_OBJS) @APP_EXTRA_LIBS@

flockDebug@EXEEXT@: $(flockDebug_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(flockDebug_OBJS) @APP_EXTRA_LIBS@

aMotionStarTest@EXEEXT@: $(aMotionStarTest_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(aMotionStarTest_OBJS) -lm

IBoxTest@EXEEXT@: $(IBoxTest_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(IBoxTest_OBJS) @APP_EXTRA_LIBS@

dummyTrackd@EXEEXT@: $(dummyTrackd_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(dummyTrackd_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

gadgetTest@EXEEXT@: $(gadgetTest_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(gadgetTest_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS) -lm

fsPinchGloveTest@EXEEXT@: $(fsPinchGloveTest_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(fsPinchGloveTest_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS) -lm

go@EXEEXT@: $(go_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(go_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

go-ibox@EXEEXT@: $(go-ibox_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(go-ibox_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

go-inputgroup@EXEEXT@: $(go-inputgroup_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(go-inputgroup_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

go-logiclass@EXEEXT@: $(go-logiclass_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(go-logiclass_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

FlockTest@EXEEXT@: $(FlockTest_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(FlockTest_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

# Suffix rules for building object files.
.SUFFIXES: .cpp .cxx .@OBJEXT@

.cpp.@OBJEXT@:
	$(CXX_COMPILE) @OBJ_NAME_FLAG@ @OBJ_BUILD_FLAG@ $<

.cxx.@OBJEXT@:
	$(CXX_COMPILE) @OBJ_NAME_FLAG@ @OBJ_BUILD_FLAG@ $<

# -----------------------------------------------------------------------------
# Clean-up targets.
# -----------------------------------------------------------------------------
clean:
	rm -f Makedepend *.@OBJEXT@ ElexolTest.ilk  FastrakTest.ilk aFlockTest.ilk aMotionStarTest.ilk IBoxTest.ilk dummyTrackd.ilk fsPinchGloveTest.ilk go.ilk go-ibox.ilk go-inputgroup.ilk go-logiclass.ilk FlockTest.ilk  so_locations *.?db core*
	rm -rf ii_files

clobber:
	@$(MAKE) clean
	rm -f ElexolTest@EXEEXT@ FastrakTest@EXEEXT@ aFlockTest@EXEEXT@ aMotionStarTest@EXEEXT@ IBoxTest@EXEEXT@ dummyTrackd@EXEEXT@ fsPinchGloveTest@EXEEXT@ go@EXEEXT@ go-ibox@EXEEXT@ go-inputgroup@EXEEXT@ go-logiclass@EXEEXT@ FlockTest@EXEEXT@ 

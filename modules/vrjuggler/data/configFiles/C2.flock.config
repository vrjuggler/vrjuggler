proxyAlias
  Name "VJHead"
  aliasName { "VJHead" }
  proxyPtr { "HeadProxy" }
  end
proxyAlias
  Name "VJWand"
  aliasName { "VJWand" }
  proxyPtr { "WandProxy" }
  end
PosProxy
  Name "HeadProxy"
  device { "Flock_o_Birds" }
  unit { 0 }
  etrans { True }
  translate { 0.167 0.0 0.271 }
  rotate { 0.0 -90.0 180.0 }
  end
PosProxy
  Name "WandProxy"
  device { "Flock_o_Birds" }
  unit { 1 }
  etrans { True }
  translate { 0.166 0.0 0.0 }
  rotate { 0.0 0.0 -90.0 }
  end
Flock
  Name "Flock_o_Birds"
  port { "/dev/ttyd3" }
  baud { 38400 }
  num { 2 }
  translate { -0.020833 7.447917 1.317708 }
  rotate { 90.0 0.0 180.0 }
  calfile { "${VJ_BASE_DIR}/share/Data/calibration.table" }
  hemi { "LOWER_HEM" }
  filt { "AC_NARROW" }
  sync { "Normal_72Hz" }
  blocking { 0 }
  report { "R" }
  transmitter { 3 }
  end
End
